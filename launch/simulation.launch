<launch>
   <include file="$(find stdr_robot)/launch/robot_manager.launch" />
   <node type="stdr_server_node" pkg="stdr_server" name="stdr_server" output="screen" args="$(find stdr_resources)/maps/simple_rooms.yaml"/>
   <node pkg="tf" type="static_transform_publisher" name="world2map" args="0 0 0 0 0 0  map world 100" />
   <include file="$(find stdr_gui)/launch/stdr_gui.launch"/>
   <node pkg="stdr_robot" type="robot_handler" name="$(anon robot_spawn)" args="add $(find reactive_navigation)/launch/pandora_robot_with_laser.yaml 1.5 1.7 0.5" />
</launch>